package org.renci.pharos.gui;

class GUISaveProviderMenuItem extends GUIMenuItem{

	/**
	 * 
	 */
	private final MainGUI mainGUI;

	public GUISaveProviderMenuItem(MainGUI mainGUI) {
		super("Save Provider");
		this.mainGUI = mainGUI;
	}

	@Override
	public void perform() {
		this.mainGUI.saveFile();			
	}
	
}