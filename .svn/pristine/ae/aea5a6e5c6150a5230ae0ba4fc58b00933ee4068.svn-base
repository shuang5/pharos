package org.renci.pharos.gui;

class GUISaveRequestMenuItem extends GUIMenuItem{

	/**
	 * 
	 */
	private final MainGUI mainGUI;

	public GUISaveRequestMenuItem(MainGUI mainGUI) {
		super("Save Request");
		this.mainGUI = mainGUI;
	}

	@Override
	public void perform() {
		this.mainGUI.saveRequest();		
	}
	
}